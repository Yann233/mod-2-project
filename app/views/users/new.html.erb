<%= form_for (@user) do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :age %>
  <%= f.text_field :age %>
  <%= f.label :img_url %>
  <%= f.text_field :img_url %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
           
  <%= f.submit "Sign Up" %>
<% end %>