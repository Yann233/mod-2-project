
<h1>Edit Your Account</h1>
<%= form_for @user do |u| %>
  <%= u.label :age %>
  <%= u.text_field :age %><br>
  <%= u.label :img_url %>
  <%= u.text_field :img_url %><br>
  <%= u.submit %>
<% end %>

<% if @current_user %>
<h4> logged in as <%= link_to @current_user.username, user_path(@current_user) %> </h4>
<% end %>

<%= form_tag sessions_path do %> 
<%= submit_tag "Log out" %> <br><br>
<% end %>

